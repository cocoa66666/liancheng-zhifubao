/* 直流桩 */
.pile_fixed {
	position: fixed;
	z-index: 10;
	top: 0;
	left: 0;
	width: 100%;
	height: 90rpx;
	background: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	text-align: center;
	border-bottom: 10rpx solid #F3F3F3;
}
