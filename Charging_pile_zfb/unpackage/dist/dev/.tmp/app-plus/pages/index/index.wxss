	/* 扩散波纹 */
@keyframes warn {
0% {
			-webkit-transform: scale(0.4);
			        transform: scale(0.4);
			opacity: 0.3;
}
25% {
			-webkit-transform: scale(0.55);
			        transform: scale(0.55);
			opacity: 0.6;
}
50% {
			-webkit-transform: scale(0.65);
			        transform: scale(0.65);
			opacity: 1;
}
75% {
			-webkit-transform: scale(0.8);
			        transform: scale(0.8);
			opacity: 1;
}
100% {
			-webkit-transform: scale(1);
			        transform: scale(1);
			opacity: 0;
}
}
@-webkit-keyframes warn {
0% {
			-webkit-transform: scale(0.4);
			        transform: scale(0.4);
			opacity: 0.3;
}
25% {
			-webkit-transform: scale(0.55);
			        transform: scale(0.55);
			opacity: 0.6;
}
50% {
			-webkit-transform: scale(0.65);
			        transform: scale(0.65);
			opacity: 1;
}
75% {
			-webkit-transform: scale(0.8);
			        transform: scale(0.8);
			opacity: 1;
}
100% {
			-webkit-transform: scale(1);
			        transform: scale(1);
			opacity: 0;
}
}

